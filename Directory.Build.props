<Project>

  <PropertyGroup>
    <Product>ForEvolve Framework</Product>
    <RepositoryUrl>https://github.com/ForEvolve/ForEvolve-Framework</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>
  <PropertyGroup>
    <ForEvolvePackagesVersion Condition="'$(FOREVOLVE_PACKAGES_VERSION)'==''">[1.0.0-upsilon-436,2.0)</ForEvolvePackagesVersion>
    <ForEvolvePhantomJsDependenciesPackagesVersion Condition="'$(FOREVOLVE_PHANTOMJS_DEPENDENCIES_PACKAGES_VERSION)'==''">1.0.0-RC-1</ForEvolvePhantomJsDependenciesPackagesVersion>
    
    <!--[2.1.2,3.0)-->
    <ForEvolveNetCoreVersion Condition="'$(FOREVOLVE_ASPNETCORE_VERSION)'==''">2.1.1</ForEvolveNetCoreVersion>
    <ForEvolveIdentityVersion Condition="'$(FOREVOLVE_IDENTITY_VERSION)'==''">2.1.2</ForEvolveIdentityVersion>
    <ForEvolveAspNetCoreTestHostVersion Condition="'$(FOREVOLVE_ASPNETCORE_TESTHOST_VERSION)'==''">2.1.1</ForEvolveAspNetCoreTestHostVersion>
    <ForEvolveXunitVersion Condition="'$(FOREVOLVE_XUNIT_VERSION)'==''">2.4.1</ForEvolveXunitVersion>
    <ForEvolveMoqVersion Condition="'$(FOREVOLVE_MOQ_VERSION)'==''">4.8.3</ForEvolveMoqVersion>
    <ForEvolveTestSdkVersion Condition="'$(FOREVOLVE_TEST_SDK_VERSION)'==''">15.8.0</ForEvolveTestSdkVersion>
  </PropertyGroup>
</Project>